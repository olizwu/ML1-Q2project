{"nbformat":4,"nbformat_minor":0,"metadata":{"colab":{"provenance":[{"file_id":"1OO-IELjEueV1oTXWGIzp8Lu7IsDSINAf","timestamp":1768782519866}],"mount_file_id":"1UdvaaVFROFvg0AURwmeZdYIKpcbHzGYe","authorship_tag":"ABX9TyPvQmngSHzEYj+nK50CvTna"},"kernelspec":{"name":"python3","display_name":"Python 3"},"language_info":{"name":"python"}},"cells":[{"cell_type":"code","execution_count":3,"metadata":{"id":"hGb0h8r5bP6Y","executionInfo":{"status":"ok","timestamp":1768920449379,"user_tz":300,"elapsed":15,"user":{"displayName":"Shreraj Singh Sainani (Student)","userId":"05641625188966813307"}}},"outputs":[],"source":["#    Updated KNN Algo without inefficient sorting   #\n","#    Taken from Lab 9                               #\n","\n","import numpy as np\n","import pandas as pd\n","import random\n","import math\n","import time\n","from sklearn.model_selection import train_test_split\n","\n","dataset =  \"/content/drive/MyDrive/ML 1 & 2/Quarter 2/Lab 9/iris.csv\"\n","df = pd.read_csv(dataset, header = 0)\n","\n","headers = list(df.columns.values)\n","atts = headers[:-1]\n","attiLookup = {header: i for i, header in enumerate(atts)}\n","labels = list(set(df[headers[-1]]))\n","labeliLookup = {label: i for i, label in enumerate(labels)}\n","\n","def splitAndSave():\n","  global train, test\n","\n","  X = df.iloc[:, :-1] # features\n","  y = df.iloc[:, -1] # target\n","\n","  X_train, X_test, y_train, y_test = train_test_split(X, y, test_size = 0.2)\n","  train = pd.concat([X_train, y_train], axis=1)\n","  train.to_csv(\"/content/drive/MyDrive/ML 1 & 2/Quarter 2/Lab 9/iris_train.csv\", index=False)\n","  test = pd.concat([X_test, y_test], axis=1)\n","  test.to_csv(\"/content/drive/MyDrive/ML 1 & 2/Quarter 2/Lab 9/iris_test.csv\", index=False)\n","\n","# splitAndSave()\n","\n","df_train = pd.read_csv(\"/content/drive/MyDrive/ML 1 & 2/Quarter 2/Lab 9/iris_train.csv\", header = 0)\n","df_test = pd.read_csv(\"/content/drive/MyDrive/ML 1 & 2/Quarter 2/Lab 9/iris_test.csv\", header = 0)\n","\n","def maxValueKey(dic):\n","  kvPairs = [(key, value) for key, value in dic.items()]\n","  maxk = kvPairs[0][0]\n","  maxv = kvPairs[0][1]\n","  for key, value in kvPairs:\n","    if value > maxv:\n","      maxv = value\n","      maxk = key\n","\n","  return maxk\n","\n","class MultiConfusionMatrix:\n","\n","  def __init__(self, CM, classes):\n","    self.CM = CM\n","    self.classes = classes\n","    self.classIndices = {j: i for i, j in enumerate(classes)}\n","    self.n = len(classes)\n","\n","  def output(self):\n","    print(\"Horizontal --> Actual ; Vertical --> Predicted\\n\")\n","    [print(str(j) + \"    \", end=\"\") for j in [\"  \"] + self.classes]\n","    print()\n","    for row in range(self.n):\n","        [print(str(j) + \"      \", end=\"\") for j in [self.classes[row]] + self.CM[row]]\n","        print()\n","    print()\n","\n","  def colSum(self, col):\n","    tot = 0\n","    for row in range(self.n):\n","      tot += self.CM[row][col]\n","\n","    return tot\n","\n","  def ALL(self):\n","    tot = 0\n","    for row in range(self.n):\n","      for col in range(self.n):\n","        tot += self.CM[row][col]\n","\n","    return tot\n","\n","  def ACC(self):\n","    T_class = 0\n","    for i in range(self.n):\n","      T_class += self.CM[i][i]\n","\n","    return T_class/self.ALL()\n","\n","  def ERR(self):\n","    return 1 - self.ACC()\n","\n","  def PREC(self, classLabel):\n","    if isinstance(classLabel, str):\n","      classLabel = self.classIndices[classLabel]\n","\n","    return self.CM[classLabel][classLabel]/(sum(self.CM[classLabel]))\n","\n","  def RCALL(self, classLabel):\n","    if isinstance(classLabel, str):\n","      classLabel = self.classIndices[classLabel]\n","\n","    return self.CM[classLabel][classLabel]/(self.colSum(classLabel))\n","\n","  def PRECAVG(self):\n","    tot = 0\n","    for _class in self.classes:\n","      tot += self.PREC(_class)\n","\n","    return tot/len(self.classes)\n","\n","  def RCALLAVG(self):\n","    tot = 0\n","    for _class in self.classes:\n","      tot += self.RCALL(_class)\n","\n","    return tot/len(self.classes)\n","\n","  def f1AVG(self):\n","    pvg = self.PRECAVG()\n","    rvg = self.RCALLAVG()\n","\n","    return (2 * pvg * rvg)/(pvg + rvg)\n","\n","  def statReport(self):\n","    print(f\"Accuracy = {self.ACC()}\")\n","    print(f\"Error = {self.ERR()}\")\n","    for _class in self.classes:\n","      print(f\"Precision: {_class} = {self.PREC(_class)}\")\n","      print(f\"Recall: {_class} = {self.RCALL(_class)}\")\n","    print(f\"F1-Score: {self.f1AVG()}\")\n","\n","class KNN_Sector_Model:\n","\n","  def __init__(self, k, train, test, nDivs):\n","\n","    self.k = k\n","    self.train = train\n","    self.test = test\n","    self.nDivs = nDivs\n","\n","    # get mins/maxes for each dimension scaling - dimensions are configured according to their order in dataframe.\n","    self.dimMins = []\n","    self.dimMaxes = []\n","    for i in range(len(atts)):\n","      self.dimMins.append(df.iloc[:, i].min())\n","      self.dimMaxes.append(df.iloc[:, i].max())\n","\n","    self.dimInequalz = [[] for dimI in range(len(atts))]\n","\n","    if self.nDivs == 1:\n","      for dimI in range(len(atts)):\n","        self.dimInequalz[dimI].append((-math.inf, math.inf))\n","    else:\n","      self.dimInequalz = [[] for i in range(len(atts))]\n","      for dimI in range(len(atts)):\n","        xnmin = self.dimMins[dimI]\n","        xnmax = self.dimMaxes[dimI]\n","        self.dimInequalz[dimI].append((-math.inf, xnmin + (xnmax-xnmin)/self.nDivs))\n","        for divI in range(1, self.nDivs-1):\n","          self.dimInequalz[dimI].append((xnmin + (divI)*(xnmax-xnmin)/self.nDivs, xnmin + (divI+1)*(xnmax-xnmin)/self.nDivs))\n","        self.dimInequalz[dimI].append((xnmin + (self.nDivs-1)*(xnmax-xnmin)/self.nDivs, math.inf))\n","\n","    self.trainingCubeAssn = {}\n","    for i in range(len(self.train)):\n","      cube = self.assignCube(self.train.iloc[i])\n","      if cube not in self.trainingCubeAssn:\n","        self.trainingCubeAssn[cube] = []\n","        self.trainingCubeAssn[cube].append(self.train.iloc[i])\n","      else:\n","        self.trainingCubeAssn[cube].append(self.train.iloc[i])\n","\n","    # self.tpDists = [[i, train.iloc[i].iloc[-1], 0] for i in range(len(train))]\n","    # self._tpDists = self.tpDists\n","\n","  def assignCube(self, point):\n","    cube = []\n","    for dimI in range(len(atts)):\n","      for Ii, inequal in enumerate(self.dimInequalz[dimI]):\n","        if inequal[0] <= point.iloc[dimI] < inequal[1]:\n","          cube.append(Ii)\n","    return tuple(cube) # see if there is a way to build up the tuple organically instead of converting at the end\n","\n","  def EuclideanDistance(self, samp1, samp2):\n","    d_sqrd = 0\n","    for att in atts:\n","      d_sqrd += (samp2[att] - samp1[att])**2\n","\n","    return math.sqrt(d_sqrd)\n","\n","  def majorityVote(self, nearestN):\n","    nearestN_ids = [nn[1] for nn in nearestN]\n","    values = set(nearestN_ids)\n","    if len(values) == 1: return list(values)[0]\n","    modeCounter = {value: 0 for value in values}\n","    for nn in nearestN_ids:\n","      modeCounter[nn] += 1\n","\n","    return maxValueKey(modeCounter)\n","\n","  def classify(self, sample): # updated to no longer use inefficient sorting.\n","\n","    sampleCube = self.assignCube(sample)\n","\n","    if sampleCube not in self.trainingCubeAssn:\n","      return random.choice(labels) # if there are no points, just take a guess.\n","\n","    sampleCubePts = self.trainingCubeAssn[sampleCube]\n","\n","    if len(sampleCubePts) < self.k: # what we do if there are not enough points in the sample cube. Still haven't implemented if there are NO points in the cube yet.\n","      kNearestN = [[i, sampleCubePts[i].iloc[-1], self.EuclideanDistance(sample, sampleCubePts[i])] for i in range(len(sampleCubePts))]\n","      return self.majorityVote(kNearestN)\n","\n","    kNearestN = [[-1, \"NullString\", math.inf] for k in range(self.k)]\n","    for i in range(len(sampleCubePts)):\n","      dist = self.EuclideanDistance(sample, sampleCubePts[i])\n","      pInQ = [i, sampleCubePts[i].iloc[-1], dist]\n","\n","      # if its actually close enough to reorder the list.... O(k)\n","      if dist < kNearestN[-1][2]:\n","        kNearestN.append(pInQ)\n","        kNearestN.sort(key=lambda x: x[2])\n","        kNearestN = kNearestN[:self.k]\n","\n","    return self.majorityVote(kNearestN)\n","\n","  def runTest(self):\n","    CM = [[0 for label in labels] for label in labels]\n","    for ti in range(len(self.test)):\n","      CM[labeliLookup[self.classify(self.test.iloc[ti])]][labeliLookup[self.test.iloc[ti].iloc[-1]]] += 1\n","\n","    CM1 = MultiConfusionMatrix(CM, labels) # relies on class order to be consistent\n","    return CM1\n","\n","  def chooseK(self, minK=1, maxK=int(0.15 * len(df_train))): # uses a global anyway instead of the class' train.\n","    self.k = minK\n","    bestk = minK\n","    CM1 = self.runTest()\n","    maxfscore = CM1.f1AVG()\n","    for kCand in range(2, maxK):\n","      self.k = kCand\n","      CM1 = self.runTest()\n","      fscore = CM1.f1AVG()\n","      if fscore > maxfscore:\n","        maxfscore = fscore\n","        bestk = kCand\n","\n","    self.k = bestk\n","    return bestk\n","\n","def main():\n","  t0_overhead = time.time()\n","  m1 = KNN_Sector_Model(4, df_train, df_test, 4)\n","  tf_overhead = time.time()\n","  print(f\"Overhead time: {tf_overhead - t0_overhead} s\")\n","  # print(f\"K Chosen: {m1.chooseK(3, 10)}\")\n","\n","  t0 = time.time()\n","  CM1 = m1.runTest()\n","  tf = time.time()\n","  print(f\"Time: {tf - t0}\")\n","  CM1.output()\n","  CM1.statReport()\n","\n","if __name__ == \"__main__\":\n","  main()"]},{"cell_type":"code","source":["#    Verify with sklearn    #\n","\n","from sklearn.neighbors import KNeighborsClassifier\n","from sklearn.metrics import accuracy_score, classification_report, precision_score, recall_score, f1_score\n","from sklearn.metrics import confusion_matrix\n","from sklearn.metrics import ConfusionMatrixDisplay\n","\n","labelCol = \"class\"\n","X_train = df_train.drop(columns=[labelCol])\n","y_train = df_train[labelCol]\n","X_test = df_test.drop(columns=[labelCol])\n","y_test = df_test[labelCol]\n","\n","# can scale (normalize) features, but not going to here to match conditions as Part 1.\n","#scaler = StandardScaler()\n","#X_train_scaled = scaler.fit_transform(X_train)\n","#X_test_scaled = scaler.transform(X_test)\n","\n","knn = KNeighborsClassifier(\n","n_neighbors=3, weights=\"distance\", metric=\"euclidean\")\n","knn.fit(X_train, y_train)\n","\n","# Predict on test set\n","y_pred = knn.predict(X_test)\n","print(\"Accuracy:\", accuracy_score(y_test, y_pred))\n","print(\"F1 Score:\", f1_score(y_test, y_pred, average=\"macro\")) # f1 score is not weighted by class\n","print(classification_report(y_test, y_pred))\n","print(confusion_matrix(y_test, y_pred))\n","\n","class_names = knn.classes_\n","\n","disp = ConfusionMatrixDisplay(\n","    confusion_matrix=confusion_matrix(y_test, y_pred, labels=class_names),\n","    display_labels=class_names\n",")\n","\n","disp.plot()"],"metadata":{"id":"XC2NFqhXd6Ar","colab":{"base_uri":"https://localhost:8080/","height":733},"executionInfo":{"status":"ok","timestamp":1768847998704,"user_tz":300,"elapsed":407,"user":{"displayName":"Shreraj Singh Sainani (Student)","userId":"05641625188966813307"}},"outputId":"5ed5f1fe-d052-4f40-b4d0-586fffed6c13"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Accuracy: 0.9333333333333333\n","F1 Score: 0.9361111111111112\n","                 precision    recall  f1-score   support\n","\n","    Iris-setosa       1.00      1.00      1.00         7\n","Iris-versicolor       0.88      0.88      0.88         8\n"," Iris-virginica       0.93      0.93      0.93        15\n","\n","       accuracy                           0.93        30\n","      mac